{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "mpicc build Lab 4",
      "type": "shell",
      "command": "mpicxx",
      "args": [
        "-g",
        "Code/main.cpp",
        "Code/IO.c",
        "Code/utils.c",
        "-o",
        "Code/main"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
